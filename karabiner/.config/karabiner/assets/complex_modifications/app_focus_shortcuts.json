{
  "title": "App Focus Shortcuts",
  "rules": [
    {
      "description": "Focus Safari (Cmd+Opt+1)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "1",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Safari'"
            }
          ]
        }
      ]
    },
    {
      "description": "Focus Finder (Cmd+Opt+F) - Example for another app",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": [
                "left_command",
                "left_option"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Finder'"
            }
          ]
        }
      ]
    }
  ]
}